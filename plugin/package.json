{
  "name": "reearth-plugin-plateauview",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "main": "./dist/reearth-plugin-plateauview.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start:datacatalog": "vite -c vite.config.datacatalog-web.ts",
    "start:sidebar": "vite -c vite.config.sidebar-web.ts",
    "build": "run-s typecheck clean build:web build:plugin",
    "build:plugin": "run-s build:sidebar build:datacatalog",
    "build:datacatalog": "vite build -c vite.config.datacatalog.ts",
    "build:sidebar": "vite build -c vite.config.sidebar.ts",
    "build:web": "run-s clean:build build:datacatalog-web build:sidebar-web --emptyOutDir false",
    "build:datacatalog-web": "vite build -c vite.config.datacatalog-web.ts --emptyOutDir false",
    "build:sidebar-web": "vite build -c vite.config.sidebar-web.ts --emptyOutDir false",
    "clean": "del dist",
    "clean:web": "del dist/web",
    "typecheck": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "type": "tsc"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.2",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^2.0.0",
    "antd": "^4.22.8",
    "del-cli": "^5.0.0",
    "eslint": "^8.21.0",
    "eslint-config-reearth": "^0.1.0",
    "jsdom": "^20.0.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "msw": "^0.44.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.7.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-singlefile": "^0.11.0",
    "vite-plugin-svgr": "^2.2.1",
    "vitest": "^0.20.3"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "msw": {
    "workerDirectory": "web/mock"
  }
}